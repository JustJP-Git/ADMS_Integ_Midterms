<Window x:Class="ADMS_Integ_Midterms.HomeStudentLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADMS_Integ_Midterms"
        mc:Ignorable="d"
        Title="HomeStudentLogin" Height="195" Width="398" WindowStartupLocation="CenterScreen">
    <Grid>
        <Button x:Name="login_btn" Content="Login" HorizontalAlignment="Center" Margin="0,116,0,0" VerticalAlignment="Top" Width="218" Click="login_btn_Click"/>
        <Label Content="Input Student ID" HorizontalAlignment="Left" Margin="34,54,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="studentid_txt" HorizontalAlignment="Left" Margin="137,58,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="215"/>
    </Grid>
</Window>
